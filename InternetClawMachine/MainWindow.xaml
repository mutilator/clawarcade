                        <Label Content="Last EPC Scan" HorizontalAlignment="Left" Height="28" Margin="189,319,0,0" VerticalAlignment="Top" Width="95" />
                        <TextBox x:Name="txtPLushName" Height="29" TextWrapping="Wrap" Text="" Margin="284,352,215,15" />
                        <Label Content="Plush Name" HorizontalAlignment="Left" Height="28" Margin="189,352,0,0" VerticalAlignment="Top" Width="95" />
                        <Button x:Name="btnPlushAddNew" Content="Add" HorizontalAlignment="Left" Margin="551,319,0,0" VerticalAlignment="Top" Width="81" Click="BtnPlushAddNew_Click" />
