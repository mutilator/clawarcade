                        <Button x:Name="btnDoh" Content="Doh" HorizontalAlignment="Left" VerticalAlignment="Top" Width="92" Margin="477,10,0,0" Click="BtnDoh_Click" />
                        <Button x:Name="btnTrombone" Content="Sad Trombone" HorizontalAlignment="Left" VerticalAlignment="Top" Width="92" Margin="477,47,0,0" Click="BtnTrombone_Click" />
                        <Button x:Name="btnScare" Content="Scare" HorizontalAlignment="Left" VerticalAlignment="Top" Width="92" Margin="477,84,0,0" Click="BtnScare_Click" />
                        <Label Content="Email From" HorizontalAlignment="Left" Margin="224,175,0,0" VerticalAlignment="Top" />
                        <TextBox HorizontalAlignment="Left" Height="23" Margin="328,178,0,0" TextWrapping="NoWrap"  Text="{Binding MailFrom}" VerticalAlignment="Top" Width="120" />
                        <Label Content="Email To" HorizontalAlignment="Left" Margin="224,206,0,0" VerticalAlignment="Top" />
