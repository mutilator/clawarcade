                        <Label Content="Tic-Tac-Toe Winner" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="157,7,0,0" />
                        <Button x:Name="btnWinnerPlayer2" Content="Player 2" HorizontalAlignment="Left" VerticalAlignment="Top" Width="84" Click="BtnWinnerPlayer2_Click" Margin="246,33,0,0" />
                        <ListBox x:Name="lstPlayerQueue" HorizontalAlignment="Left" Height="185" VerticalAlignment="Top" Width="232" Margin="519,41,0,0" BorderBrush="#FF6D6D6D" BorderThickness="1" />
