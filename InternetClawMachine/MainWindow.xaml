                        <Label Content="VoteDuration" HorizontalAlignment="Left" Margin="10,264,0,0" VerticalAlignment="Top" RenderTransformOrigin="-0.026,-0.077" />
                        <Label x:Name="lblvotesNeededForVotingMode_Copy" HorizontalAlignment="Left" Margin="169,264,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.368,-0.308" Content="{Binding Value, ElementName=sldrvotesNeededForVotingMode_Copy}" />
                        <Button x:Name="btnDoh" Content="Doh" HorizontalAlignment="Left" VerticalAlignment="Top" Width="162" Margin="477,10,0,0" Click="BtnDoh_Click" />
                        <Button x:Name="btnTrombone" Content="Sad Trombone" HorizontalAlignment="Left" VerticalAlignment="Top" Width="162" Margin="477,47,0,0" Click="BtnTrombone_Click" />
                        <Button x:Name="btnScare" Content="Scrm" HorizontalAlignment="Left" VerticalAlignment="Top" Width="65" Margin="477,84,0,0" Click="BtnScare_Click" />
                        <Label Content="Email From" HorizontalAlignment="Left" Margin="224,175,0,0" VerticalAlignment="Top" />
                        <TextBox HorizontalAlignment="Left" Height="23" Margin="328,178,0,0" TextWrapping="NoWrap"  Text="{Binding MailFrom}" VerticalAlignment="Top" Width="120" />
                        <Label Content="Email To" HorizontalAlignment="Left" Margin="224,206,0,0" VerticalAlignment="Top" />
                        <TextBox HorizontalAlignment="Left" Height="23" Margin="328,209,0,0" TextWrapping="NoWrap" Text="{Binding EmailAddress}" VerticalAlignment="Top" Width="120" />
                        <ComboBox x:Name="cmbLogLevel" HorizontalAlignment="Left" VerticalAlignment="Top" Width="120" Margin="328,241,0,0" SelectionChanged="CmbLogLevel_SelectionChanged" DisplayMemberPath="Name" />
                        <Label Content="Log Level" HorizontalAlignment="Left" Margin="224,241,0,0" VerticalAlignment="Top" />
                        <Button x:Name="btnReloadTranslations" Content="Reload Translations" HorizontalAlignment="Left" VerticalAlignment="Top" Width="162" Margin="477,290,0,0" Click="BtnReloadTranslations_Click" />
                        <Button x:Name="btnTvEnable" Content="Enable TV" HorizontalAlignment="Left" VerticalAlignment="Top" Width="108" Margin="477,180,0,0" Click="BtnTvEnable_Click" />
                        <Button x:Name="btnTvDisable" Content="Disable TV" HorizontalAlignment="Left" VerticalAlignment="Top" Width="110" Margin="590,180,0,0" Click="BtnTvDisable_Click" />
                        <Button x:Name="btnGlitch1" Content="1" HorizontalAlignment="Left" VerticalAlignment="Top" Width="45" Margin="477,143,0,0" />
                        <Button x:Name="btnGlitch2" Content="2" HorizontalAlignment="Left" VerticalAlignment="Top" Width="45" Margin="527,143,0,0" />
                        <Button x:Name="btnGlitch3" Content="3" HorizontalAlignment="Left" VerticalAlignment="Top" Width="45" Margin="577,143,0,0" />
                        <Button x:Name="btnGlitch4" Content="4" HorizontalAlignment="Left" VerticalAlignment="Top" Width="45" Margin="627,143,0,0" />
                        <Label Content="Glitches" HorizontalAlignment="Left" Margin="477,121,0,0" VerticalAlignment="Top" />
                        <Button x:Name="btnGhost1" Content="Gst1" HorizontalAlignment="Left" VerticalAlignment="Top" Width="65" Margin="547,84,0,0" Click="BtnGhost1_Click" />
                        <Button x:Name="btnGhost2" Content="Gst2" HorizontalAlignment="Left" VerticalAlignment="Top" Width="65" Margin="617,84,0,0" Click="BtnGhost2_Click" />
