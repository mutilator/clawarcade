                        <CheckBox x:Name="chkPrintJob" Content="Print Job Enabled" HorizontalAlignment="Left" Margin="230,290,0,0" VerticalAlignment="Top" IsChecked="{Binding PrintJobEnabled, Mode=TwoWay}" />
                    </Grid>
                </TabItem>
                <TabItem Header="Plushes" Padding="20,2">
                    <Grid>
                        <ListView x:Name="lstPlushes" HorizontalAlignment="Left" Height="304" VerticalAlignment="Top" Width="716" Margin="10,10,0,0" GridViewColumnHeader.Click="LstPlushes_OnClick">
                            <ListView.View>
                                <GridView>
                                    <GridViewColumn DisplayMemberBinding="{Binding PlushId}" Header="Id" Width="50" />
                                    <GridViewColumn DisplayMemberBinding="{Binding Name}" Header="Plush" Width="200" />
                                    <GridViewColumn DisplayMemberBinding="{Binding WasGrabbed}" Header="Grabbed" Width="85" />
                                    <GridViewColumn DisplayMemberBinding="{Binding ChangedBy}" Header="Changed By" Width="150" />
                                    <GridViewColumn DisplayMemberBinding="{Binding ChangeDate, Converter={StaticResource epochToDate}}" Header="Changed Date" Width="120" />
                                </GridView>
                            </ListView.View>
                        </ListView>
                        <Button x:Name="btnMarkGrabbed" Content="Grabbed" HorizontalAlignment="Left" Margin="10,319,0,0" VerticalAlignment="Top" Width="88" Click="BtnMarkGrabbed_Click" />
                        <Button x:Name="btnPlushListRefresh" Content="Refresh" HorizontalAlignment="Left" Margin="103,319,0,0" VerticalAlignment="Top" Width="81" Click="BtnPlushListRefresh_Click" />
