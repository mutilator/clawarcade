                        <CheckBox Content="IR Triggers Win" HorizontalAlignment="Left" IsChecked="{Binding EventMode.IRTriggersWin, Mode=TwoWay}" VerticalAlignment="Top" Margin="217,128,0,0" />
                        <xctk:IntegerUpDown Value="{Binding EventMode.WinMultiplier, Mode=TwoWay}" Margin="10,262,700,113" />
                        <Label Content="Win Multiplier" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="66,262,0,0" />
                        <TextBox HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" Text="{Binding EventMode.CustomWinTextResource, Mode=TwoWay}" VerticalAlignment="Top" Width="78" Margin="10,201,0,0" />
