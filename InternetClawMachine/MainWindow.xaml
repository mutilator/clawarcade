                        <Slider x:Name="sldrwinNotificationDelay_Copy" HorizontalAlignment="Left" Margin="10,136,0,0" VerticalAlignment="Top" Width="113" Maximum="100" Minimum="0" LargeChange="1" SmallChange=".05" Value="{Binding GoalProgressIncrement, Mode=TwoWay}" IsSnapToTickEnabled="True" TickFrequency="250" />
