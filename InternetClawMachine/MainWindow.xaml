            <Button Content="Backward" HorizontalAlignment="Left" Margin="586,96,0,0" VerticalAlignment="Top" Width="98" x:Name="btnBackward" PreviewMouseDown="btnBackward_MouseDown" PreviewMouseUp="btnBackward_MouseUp" />
